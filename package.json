{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "panda codegen && cross-env NODE_ENV=production run-p build:*",
    "build:types": "tsc --noEmit",
    "build:remix": "remix vite:build",
    "build:server": "esbuild src/server/main.ts --bundle --format=esm --sourcemap --platform=node --target=node20 --outdir=out --packages=external --external:./build/server/index.js",
    "dev": "panda codegen && run-p dev:*",
    "dev:remix": "onchange src/server src/shared -ik -- tsx --env-file=.env --env-file=.env.local src/server/main.ts",
    "dev:db": "docker compose up",
    "dev:types": "tsc --noEmit --watch --preserveWatchOutput",
    "dev:prisma": "prisma generate --watch"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/static": "^6.12.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mcansh/remix-fastify": "^3.2.1",
    "@prisma/client": "^5.8.1",
    "@radix-ui/themes": "^2.0.3",
    "@remix-run/node": "^2.5.0",
    "@remix-run/react": "^2.5.0",
    "@simplewebauthn/browser": "^9.0.1",
    "@simplewebauthn/server": "^9.0.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "cookie": "^0.6.0",
    "cookie-signature": "^1.2.1",
    "discord.js": "^14.14.1",
    "fastify": "^4.25.2",
    "isbot": "^4.1.0",
    "ky": "^1.2.0",
    "react": "^18.2.0",
    "react-admin": "^4.16.7",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@fastify/middie": "^8.3.0",
    "@pandacss/dev": "^0.27.2",
    "@remix-run/dev": "^2.5.0",
    "@types/cookie-signature": "^1.1.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "cross-env": "^7.0.3",
    "esbuild": "0.19.12",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^3.2.4",
    "prisma": "^5.8.1",
    "tsx": "^4.7.0",
    "typescript": "~5.3.3",
    "vite": "^5.0.11"
  },
  "engines": {
    "node": "^20.11.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
