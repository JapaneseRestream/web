{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "panda codegen && cross-env NODE_ENV=production run-s build:*",
    "build:types": "tsc --noEmit",
    "build:remix": "remix vite:build",
    "build:server": "esbuild src/server/main.ts --bundle --format=esm --sourcemap --platform=node --target=node20 --outdir=out --packages=external --external:./build/server/index.js",
    "build:admin": "cd src/admin && vite build",
    "dev": "panda codegen && run-p dev:*",
    "dev:remix": "onchange src/server src/shared -ik -- tsx --env-file=.env --env-file=.env.local src/server/main.ts",
    "dev:db": "docker compose up",
    "dev:types": "tsc --noEmit --watch --preserveWatchOutput",
    "dev:prisma": "prisma generate --watch",
    "dev:admin": "cd src/admin && npx vite",
    "lint": "eslint --ext .ts,.tsx src"
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.511.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/helmet": "^11.1.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mcansh/remix-fastify": "^3.2.2",
    "@prisma/client": "^5.9.1",
    "@radix-ui/themes": "^2.0.3",
    "@remix-run/node": "^2.6.0",
    "@remix-run/react": "^2.6.0",
    "@simplewebauthn/browser": "^9.0.1",
    "@simplewebauthn/server": "^9.0.2",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.1",
    "@trpc/react-query": "^10.45.1",
    "@trpc/server": "^10.45.1",
    "cookie": "^0.6.0",
    "cookie-signature": "^1.2.1",
    "discord.js": "^14.14.1",
    "fastify": "^4.26.0",
    "isbot": "^4.4.0",
    "ky": "^1.2.0",
    "react": "^18.2.0",
    "react-admin": "^4.16.10",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2"
  },
  "devDependencies": {
    "@fastify/middie": "^8.3.0",
    "@pandacss/dev": "^0.30.2",
    "@remix-run/dev": "^2.6.0",
    "@types/cookie-signature": "^1.1.2",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "esbuild": "0.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.9.1",
    "tsx": "^4.7.1",
    "typescript": "~5.3.3",
    "vite": "^5.1.1"
  },
  "engines": {
